{% block stylesheets %}
  <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.css') }}" />
  <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="{{ asset('css/nice-select.min.css') }}">
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />
  <link href="{{ asset('css/font-awesome.min.css') }}" rel="stylesheet" />
  <link href="{{ asset('css/style.css') }}" rel="stylesheet" />
  <link href="{{ asset('css/responsive.css') }}" rel="stylesheet" />
{% endblock %}

{% block body %}
  <body>

  <div class="hero_area">
    <div class="bg-box">
        <img src="{{ asset('img/slider-bg.jpg') }}" alt="">
    </div>
    <!-- header section strats -->
    <header class="header_section">
      <div class="container">
        <nav class="navbar navbar-expand-lg custom_nav-container ">
          <a class="navbar-brand" href="{{ path('user_profile') }}">
            <span>
              E-PARKING
            </span>
          </a>

          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class=""> </span>
          </button>

          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav  ">
              <li class="nav-item active">
                <a class="nav-link" href="{{ path('user_profile') }}">Home <span class="sr-only">(current)</span></a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="{{ path('about') }}"> About</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="{{ path('pricing') }}">Pricing</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="{{ path('why') }}">Why Us</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="{{ path('testimonial') }}">reclamation</a>
              </li>
              
            </ul>
           
          </div>
        </nav>
      </div>
    </header>
    <!-- end header section -->
  </div>

  <!-- client section -->

  <section class="client_section layout_padding">
    <div class="container">
      <div class="heading_container col">
        <h2>
          What Says Our <span>Client</span>
        </h2>

             
	<div class="container">
		<div class="row">
			<h3 class="mt-5 mb-5">Remplir une Reclamation</h3>

			<div class="col-md-6 offset-md-3">
				{{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
				<div class="form-group">
					{{ form_label(form.nom_client)}}
					{{ form_widget(form.nom_client,{'attr':{'class':'form-control'}})}}
					{{ form_errors(form.nom_client) }}
				</div>

				<div class="form-group">
					{{ form_label(form.sujet)}}
					{{ form_widget(form.sujet,{'attr':{'class':'form-control'}})}}
					{{ form_errors(form.sujet) }}
				</div>
				<div class="form-group">
					{{ form_label(form.etat)}}
					{{ form_widget(form.etat,{'attr':{'class':'form-control'}})}}
					{{ form_errors(form.etat) }}
				</div>
				<div
					class="form-group">
					<!-- Utilisation de la classe btn-primary pour appliquer un style Bootstrap au bouton -->
					<button class="btn btn-primary mt-2">Submit</button>
				</div>

				{{ form_end(form) }}
			</div>
		</div>
	</div>

             
      </div>
      


</body>

{% endblock %}
