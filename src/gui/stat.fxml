<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.StatController">
    <scene>
        <Scene>
            <AnchorPane fx:id="main_form" prefHeight="534.0" prefWidth="637.0">
            <children>
               <BarChart fx:id="barChart" layoutX="40.0" layoutY="58.0" prefHeight="413.0" prefWidth="529.0" title="Data chart(Reclamation)">
                 <xAxis>
                   <CategoryAxis side="BOTTOM" />
                 </xAxis>
                 <yAxis>
                   <NumberAxis side="LEFT" />
                 </yAxis>
               </BarChart>
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
